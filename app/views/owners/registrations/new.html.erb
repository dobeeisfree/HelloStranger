<div class="ui grid owner-register-form">

  <div class="four wide column" id="status">
    <div class="ui vertical steps">
      <div class="active step">
        <div class="content">
          <div class="title">1단계</div>
          <div class="description">오너 계정을 설정합니다.</div>
        </div>
      </div>
      <div class="disabled step">
        <div class="content">
          <div class="title">2단계</div>
          <div class="description">하나의 매장을 등록합니다.</div>
        </div>
      </div>
    </div>
  </div>

  <div class="eight wide column owner-account">
    <div class="ui attached message">
      <div class="header">
        오너님 환영합니다!
      </div>
      <p>매장 등록은 "매장 현황"에서 해주세요</p>
    </div>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html:{class: "ui form attached fluid segment"}) do |f| %>
      <%= devise_error_messages! %>
      <%= render partial: "layouts/ownerform", locals: {f: f, owner_edit: false} %>
    <% end %>
  </div>

</div>
