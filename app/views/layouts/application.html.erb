<!DOCTYPE html>
<html>
<head>
  <title>HelloStranger</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= favicon_link_tag 'logo' %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
  <link rel="stylesheet" type="text/css" href="/semantic/dist/semantic.min.css">
  <script src="/semantic/dist/semantic.min.js"></script>
  <%= csrf_meta_tags %>
</head>

  <body id="<%=controller.controller_name%>-<%=controller.action_name%>">
    <%= render partial: "layouts/header" unless controller.controller_name == 'sessions' %>
    <%= render partial: "layouts/sessionNew" %>
    <%= yield %>
  </body>
</html>
